	createNav("dims.children", "dims.siblings", "dims.visually-mapped", "gray");
	var root = createRoot(200, 250, "comes");
	var root2 = createRoot(400, 400, "root", false);
	createCellYNeg(root, "Zalgo");
	var cell = root;
	var cells = ["he", "say", "they", "qtzz"];
	for (c in cells)
	{
		cell = createCellXNeg(cell, cells[c]);
	}

	createCellYNeg(root2, "something");
	createCellXPos(root2, "hello there");
	createCellYPos(root2, "else");
}

		</script>

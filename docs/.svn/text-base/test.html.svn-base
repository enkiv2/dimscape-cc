		<script type="text/javascript;version=1.8">
function figs()
{
	fig1();
	fig2();
}
function fig1()
{
	var ctz = getFigure("fig1");
	createNav(".qtzz.bye", ".qtzz.they.say.he.comes", ".qtzz.hello", "tan");
